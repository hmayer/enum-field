{
    "name": "hmayer/enum-field",
    "type": "library",
    "description": "A Laravel Nova field for PHP Enum classes.",
    "keywords": [
        "laravel",
        "nova",
        "enum"
    ],
    "license": "MIT",
    "require": {
        "php": ">=8.1.0",
        "laravel/nova": "^4.0"
    },
    "autoload": {
        "psr-4": {
            "Hmayer\\EnumField\\": "src/"
        }
    },
    "extra": {
        "laravel": {
            "providers": []
        }
    },
    "config": {
        "sort-packages": true
    },
    "minimum-stability": "dev",
    "prefer-stable": true,
    "repositories": [
        {
            "type": "composer",
            "url": "https://nova.laravel.com"
        }
    ]
}
